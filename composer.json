{
  "name": "jason-napolitano/project-template",
  "type": "library",
  "description": "A simple template for a Github/Packgist repository. Built to implement Travis-CI, PSR-4 and PHPUnit 8! ",
  "homepage": "https://github.com/jason-napolitano/Project-Template",
  "license": "MIT",
  "version": "0.0.1",
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
      "phpunit/phpunit": "^9.5",
      "squizlabs/php_codesniffer": "^3.7.1"
  },
  "autoload": {
    "psr-4": {
      "NamespaceName\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "./tests"
    }
  }
}
